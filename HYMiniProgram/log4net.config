      <file value="logs\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyy-MM-dd/'fatal.txt'" />
      <!--设置无限备份=-1 ，最大备份数为30-->
      <maxSizeRollBackups value="30"/>
      <!--每个文件的大小-->
